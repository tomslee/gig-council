{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_FIREBASE_EMAIL": "$EXPO_PUBLIC_FIREBASE_EMAIL",
        "EXPO_PUBLIC_FIREBASE_PASSWORD": "$EXPO_PUBLIC_FIREBASE_PASSWORD"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
